<% tabset ||= 'tabs' %>
<% if editor? || item.has_tags? %>
  <div id="<%= tabset %>_tags">
	  <h2 class="boxhead">tags</h2>
    	<ul class="starry">
    	<% if item.has_tags? %>
    		<%= render :partial => "tags/minilist", :collection => item.tags %>
    	<% end %>
    	<% if editor? %>
    	  <li class="add"><strike><%= link_to "add a tag", '#' %></strike></li>
      <% end %>
    </ul>
	</div>
<% end %>
