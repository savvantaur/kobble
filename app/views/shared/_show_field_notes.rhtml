<% tabset ||= 'tabs' %>
<% if item.has_notes? %>
  <div id="<%= tabset %>_notes">
		<% unless item.observations.nil? || item.observations.size == 0 %>
			<%= "*Observations*: #{item.observations}".formatted %>
		<% end %>
		<% unless item.emotions.nil? || item.emotions.size == 0 %>
			<%= "*Emotions*: #{item.emotions}".formatted %>
		<% end %>
		<% unless item.arising.nil? || item.arising.size == 0 %>
			<%= "*Connections*: #{item.arising}".formatted %>
		<% end %>
	</div>
<% end %>
