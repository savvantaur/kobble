<% form_for :scratchpad,
            :url => scratchpad_path(@scratchpad),
            :html => { 
              :method => "post",
              :onsubmit => 'return false;'
            } do |f| 
%>
  <%= render :partial => 'form' %>
  <div class="formbuttons">
    <%= submit_tag "create pad" %> or
    <%= link_to 'cancel', '#', :class => 'remove_tab' %>
  </div>
<% end %>
